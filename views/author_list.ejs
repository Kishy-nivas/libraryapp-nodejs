<html
	<head>
		<title> Library App <%= title %> </title> 
	</head> 
	 <%- include('./partials/script'); -%> 
	 <%- include('./partials/navbar'); -%> 
	 <%- include('./partials/side_navbar'); -%>
	 <hr> 
	 <body class ="bg-white"> 
	 <div class = "container">
	 	  <div class = "row">
	 	    <div class = "col-md-9 offset-md-2">
	 	        <h3 class = "text-primary"> <%= title %> </h3>
	 	        <ul class = "list-group">
	 	        <% author_list.forEach(function(author){ %> 
	 	            <li class = "list-group-item"> <a href = "<%= author.url %>" > <%= author.first_name %></a> <%= author.date_of_birth %> - <%= author.date_of_death %>)</li>
	 	       <% }); %> 
	 		</div>
	 	 </div>
	 </div> 
	 
	 
	 </body> 
</html> 
